{
  "source": {
    "include": ["./src"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
    "plugins/markdown",
    "node_modules/better-docs/category"
  ],
  "opts": {
    "encoding": "utf8",
    "readme": "./docs.md",
    "destination": "docs/",
    "recurse": true,
    "template": "node_modules/better-docs",
    "tutorials": "tutorials/"
  }
}