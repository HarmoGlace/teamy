{
  "source": {
    "include": [
      "./src"
    ],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
    "plugins/markdown",
    "node_modules/better-docs/category"
  ],
  "opts": {
    "encoding": "utf8",
    "readme": "./docs.md",
    "destination": "docs/",
    "recurse": true,
    "template": "node_modules/better-docs",
    "tutorials": "tutorials/"
  },
  "templates": {
    "search": true,
    "better-docs": {
      "name": "Teamy documentation",
      "title": "Teamy |",
      "hideGenerator": true,
      "navLinks": [
        {
          "label": "Github",
          "href": "https://github.com/HarmoGlace/teamy/"
        },
        {
          "label": "Discord",
          "href": "https://discord.gg/CnuKPMM"
        }
      ]
    }
  }
}